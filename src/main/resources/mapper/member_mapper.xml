<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.cod.app.member.mapper.MemberMapper">

<insert id="insertMember" parameterType="co.cod.app.member.MemberVO">
	INSERT INTO MEMBER	(
	EMAIL, PW, BIRTHDAY, PHONE, NICKNAME
	)
	VALUES(
	#{email},
	#{pw},
	#{birthday},
	#{phone},
	#{nickname}
	)
	</insert>
	
	
	<select id = "getMemberList" parameterType="co.cod.app.member.MemberVO" resultType="co.cod.app.member.MemberVO">
		
		select * from member
	</select>
	
	
</mapper>